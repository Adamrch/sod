{
    "compilerOptions": {
        "target": "ES2021",
        "module": "es2015",
        "moduleResolution": "node",
        "strict": true,
        "lib": [
            "DOM",
            "ES2021",
            "DOM.Iterable"
        ],
        "types": [],
        "outDir": "dist/sod",
        "pretty": false,
        "allowJs": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "baseUrl": ".",
        "jsx": "react",
        "jsxFactory": "element",
        "jsxFragmentFactory": "fragment",
    },
    "exclude": [
        "dist/sod",
        "assets",
        "sim",
        "proto"
    ]
}
